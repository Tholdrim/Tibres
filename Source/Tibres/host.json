{
  "version": "2.0",
  "extensions": {
    "queues": {
      "maxPollingInterval": "00:00:15"
    }
  },
  "logging": {
    "applicationInsights": {
      "enablePerformanceCountersCollection": false,
      "samplingSettings": {
        "isEnabled": false
      }
    },
    "logLevel": {
      "Default": "Warning",
      "Host.Results": "Information",
      "Microsoft.Azure.WebJobs.Hosting.JobHostService": "Information"
    }
  }
}
